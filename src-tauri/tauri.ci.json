{
  "tauri": {
    "bundle": {
      "windows": {
        "digestAlgorithm": "sha256",
        "timestampUrl": "http://timestamp.digicert.com",
        "signCommand": "powershell -NoProfile -Command \"& 'signtool.exe' sign /fd sha256 /f $env:TAURI_WIN_CERT_PATH /p $env:TAURI_WIN_CERT_PASS /tr http://timestamp.digicert.com /td sha256 $path\""
      }
    }
  }
}
